{
  "name": "mongoose-jsonld",
  "version": "0.1.27",
  "description": "Add JSON-LD context to a Mongoose schema",
  "main": "lib/index.js",
  "scripts": {
    "test": "for i in test/*.coffee;do coffee $i|tap-spec;done",
    "mongo": "mongod --dbpath data/mongodb --port 27018",
    "docs": "docker -i src -o apidocs -s -n -c vs",
    "compile": "coffee -c -o lib src",
    "clean": "rm -r apidocs lib ; true",
    "prepublish": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kba/mongoose-jsonld.git"
  },
  "author": "kba",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kba/mongoose-jsonld/issues"
  },
  "homepage": "https://github.com/kba/mongoose-jsonld",
  "devDependencies": {
    "body-parser": "^1.12.2",
    "docker": "^0.2.14",
    "express": "^4.12.3",
    "superagent": "^1.1.0",
    "supertest": "^0.15.0",
    "tap-spec": "^3.0.0",
    "tape": "^4.0.0",
    "tapes": "^2.0.0"
  },
  "dependencies": {
    "async": "^0.9.2",
    "mongoose": "^4.1.9",
    "express-jsonld": "0.0.14",
    "jsonld-common-contexts": "0.0.10",
    "jsonld-rapper": "0.0.14",
    "merge": "^1.2.0",
    "node-uuid": "^1.4.3"
  }
}
